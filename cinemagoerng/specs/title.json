{
  "url": "https://www.imdb.com/title/tt%(imdb_id)s/",
  "rules": {
    "next_data": {
      "path": "//script[@id='__NEXT_DATA__']/text()",
      "transform": "json",
      "post_map": {
        "type": "props.pageProps.aboveTheFoldData.titleType.id",
        "title": "props.pageProps.aboveTheFoldData.titleText.text",
        "year": "props.pageProps.aboveTheFoldData.releaseYear.year",
        "end_year": "props.pageProps.aboveTheFoldData.releaseYear.endYear",
        "genres": "props.pageProps.aboveTheFoldData.genres.genres[*].text"
      }
    }
  }
}
