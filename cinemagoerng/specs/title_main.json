{
  "url": "https://www.imdb.com/title/%(imdb_id)s/",
  "rules": [
    {
      "key": "json",
      "extractor": {
        "xpath": "//script[@id='__NEXT_DATA__']/text()",
        "transform": "json"
      },
      "skip": true,
      "post_map": [
        {
          "key": "imdb_id",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.id"
          }
        },
        {
          "key": "type_id",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.titleType.id"
          }
        },
        {
          "key": "title",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.originalTitleText.text"
          }
        },
        {
          "key": "year",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.releaseYear.year"
          }
        },
        {
          "key": "end_year",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.releaseYear.endYear"
          }
        },
        {
          "key": "runtime",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.runtime.seconds",
            "transform": "div60"
          }
        },
        {
          "key": "rating",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.ratingsSummary.aggregateRating",
            "transform": "decimal"
          }
        },
        {
          "key": "vote_count",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.ratingsSummary.voteCount"
          }
        },
        {
          "key": "plot.lang",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.plot.language.id"
          }
        },
        {
          "key": "plot",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.plot.plotText.plainText",
            "transform": "lang"
          }
        },
        {
          "key": "genres",
          "extractor": {
            "jmespath": "props.pageProps.aboveTheFoldData.genres.genres[*].text"
          }
        }
      ]
    }
  ]
}
