{
  "url": "https://www.imdb.com/title/tt%(imdb_id)s/taglines/",
  "rules": {
    "next_data": {
      "path": "//script[@id='__NEXT_DATA__']/text()",
      "transform": "json",
      "post_map": {
        "taglines": {
          "path": "props.pageProps.contentData.section.items[*].htmlContent",
          "transform": "unescape"
        }
      }
    }
  }
}
