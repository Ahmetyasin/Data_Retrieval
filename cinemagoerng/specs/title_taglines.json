{
  "url": "https://www.imdb.com/title/%(imdb_id)s/taglines/",
  "rules": [
    {
      "key": "_json",
      "extractor": {
        "path": "//script[@id='__NEXT_DATA__']/text()",
        "transform": "json",
        "post_map": [
          {
            "key": "taglines",
            "extractor": {
              "path": "props.pageProps.contentData.section.items[*].htmlContent",
              "transform": "unescape*"
            }
          }
        ]
      }
    }
  ]
}
