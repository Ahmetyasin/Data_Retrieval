{
  "url": "https://www.imdb.com/title/%(imdb_id)s/taglines/",
  "rules": [
    {
      "key": "json",
      "extractor": {
        "xpath": "//script[@id='__NEXT_DATA__']/text()",
        "transform": "json"
      },
      "skip": true,
      "post_map": [
        {
          "key": "taglines",
          "extractor": {
            "jmespath": "props.pageProps.contentData.section.items[*].htmlContent",
            "transform": "unescape"
          }
        }
      ]
    }
  ]
}
