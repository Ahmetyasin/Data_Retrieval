{
  "version": "20240103",
  "url": "https://www.imdb.com/title/%(imdb_id)s/taglines/",
  "rules": [
    {
      "key": "taglines",
      "extractor": {
        "pre": "next_data",
        "foreach": "/NEXT_DATA/contentData/section/items/item",
        "path": "./htmlContent/text()",
        "transform": "unescape"
      }
    }
  ]
}
