{
  "version": "20240205",
  "url": "https://www.imdb.com/title/%(imdb_id)s/episodes/?season=%(season)s",
  "doctype": "html",
  "rules": [
    {
      "key": "imdb_id",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/id/text()"
      }
    },
    {
      "key": "type_id",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/titleType/id/text()"
      }
    },
    {
      "key": "title",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/originalTitleText/text/text()"
      }
    },
    {
      "key": "primary_image",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/primaryImage/url/text()"
      }
    },
    {
      "key": "year",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/releaseYear/year/text()",
        "transform": "int"
      }
    },
    {
      "key": "end_year",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/releaseYear/endYear/text()",
        "transform": "int"
      }
    },
    {
      "key": "episode_count",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/section/episodesWidgetData/totalEpisodes/total/text()",
        "transform": "int"
      }
    },
    {
      "key": "episodes",
      "extractor": {
        "pre": "next_data",
        "rules": [
          {
            "foreach": "/NEXT_DATA/contentData/section/currentSeason",
            "key": {
              "path": "./text()"
            },
            "extractor": {
              "foreach": "../episodes/items/item",
              "rules": [
                {
                  "key": "imdb_id",
                  "extractor": {
                    "path": "./id/text()"
                  }
                },
                {
                  "key": "type_id",
                  "extractor": {
                    "path": "./type/text()"
                  }
                },
                {
                  "key": "title",
                  "extractor": {
                    "path": "./titleText/text()"
                  }
                },
                {
                  "key": "season",
                  "extractor": {
                    "path": "./season/text()"
                  }
                },
                {
                  "key": "episode",
                  "extractor": {
                    "path": "./episode/text()"
                  }
                },
                {
                  "key": "year",
                  "extractor": {
                    "path": "./releaseYear/text()",
                    "transform": "int"
                  }
                },
                {
                  "key": "release_date",
                  "extractor": {
                    "rules": [
                      {
                        "key": "year",
                        "extractor": {
                          "path": "./releaseDate/year/text()",
                          "transform": "int"
                        }
                      },
                      {
                        "key": "month",
                        "extractor": {
                          "path": "./releaseDate/month/text()",
                          "transform": "int"
                        }
                      },
                      {
                        "key": "day",
                        "extractor": {
                          "path": "./releaseDate/day/text()",
                          "transform": "int"
                        }
                      }
                    ],
                    "transform": "date"
                  }
                },
                {
                  "key": "plot",
                  "extractor": {
                    "rules": [
                      {
                        "key": "key",
                        "extractor": {
                          "path": "/NEXT_DATA/translationContext/i18n/locale/text()"
                        }
                      },
                      {
                        "key": "value",
                        "extractor": {
                          "path": "./plot/text()"
                        }
                      }
                    ],
                    "transform": "make_dict"
                  }
                },
                {
                  "key": "rating",
                  "extractor": {
                    "path": "./aggregateRating/text()",
                    "transform": "decimal"
                  }
                },
                {
                  "key": "vote_count",
                  "extractor": {
                    "path": "./voteCount/text()",
                    "transform": "int"
                  }
                }
              ],
              "post": "episode_map"
            }
          }
        ]
      }
    }
  ]
}
