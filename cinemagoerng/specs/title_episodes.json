{
  "version": "20240126",
  "url": "https://www.imdb.com/title/%(imdb_id)s/episodes/?season=%(season)s",
  "rules": [
    {
      "key": "imdb_id",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/id/text()"
      }
    },
    {
      "key": "type_id",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/titleType/id/text()"
      }
    },
    {
      "key": "title",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/originalTitleText/text/text()"
      }
    },
    {
      "key": "primary_image",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/primaryImage/url/text()"
      }
    },
    {
      "key": "year",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/releaseYear/year/text()",
        "transform": "int"
      }
    },
    {
      "key": "end_year",
      "extractor": {
        "pre": "next_data",
        "path": "/NEXT_DATA/contentData/entityMetadata/releaseYear/endYear/text()",
        "transform": "int"
      }
    },
    {
      "key": "episodes",
      "extractor": {
        "pre": "next_data",
        "foreach": "/NEXT_DATA/contentData/section/episodes/items/item",
        "rules": [
          {
            "key": "imdb_id",
            "extractor": {
              "path": "./id/text()"
            }
          },
          {
            "key": "type_id",
            "extractor": {
              "path": "./type/text()"
            }
          },
          {
            "key": "title",
            "extractor": {
              "path": "./titleText/text()"
            }
          },
          {
            "key": "season",
            "extractor": {
              "path": "./season/text()"
            }
          },
          {
            "key": "episode",
            "extractor": {
              "path": "./episode/text()"
            }
          }
        ]
      }
    }
  ]
}
